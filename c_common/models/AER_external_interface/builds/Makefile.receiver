CFLAGS += -DRECEIVER 

$(BUILD_DIR)$(PRAERIE_SOURCE_DIR)Timer.o : TARGET_INCLUDES += $(PRAERIE_SOURCE_DIR)DMA_receive_buffer.h
$(BUILD_DIR)$(PRAERIE_SOURCE_DIR)PRAERIE_processor.o : TARGET_INCLUDES += $(PRAERIE_SOURCE_DIR)SDP_receive.h $(PRAERIE_SOURCE_DIR)DMA_receive_buffer.h
$(BUILD_DIR)$(PRAERIE_SOURCE_DIR)SDP_receive.o : TARGET_INCLUDES = $(PRAERIE_SOURCE_DIR)DMA_receive_buffer.h
$(BUILD_DIR)$(PRAERIE_SOURCE_DIR)DMA_receive_buffer.o : TARGET_INCLUDES = $(PRAERIE_SOURCE_DIR)SDP_receive.h $(PRAERIE_SOURCE_DIR)Command_processor.h PRAERIE_codec.h

SOURCES += $(PRAERIE_SOURCE_DIR)SDP_receive.c $(PRAERIE_SOURCE_DIR)DMA_receive_buffer.c
